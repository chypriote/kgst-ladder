    "@nuxtjs/stylelint-module": "^4.0",
    "@types/lodash-es": "^4.17",
    "@types/numeral": "^2.0",
    "eslint": "^7.31",
    "eslint-config-prettier": "^8.3",
    "eslint-plugin-nuxt": "^2.0",
    "eslint-plugin-prettier": "^3.4",
    "eslint-plugin-vue": "^7.14",
    "husky": "^7.0",
    "lint-staged": "^11.1",
    "prettier": "^2.3",
    "stylelint": "^13.13",
    "stylelint-config-prettier": "^8.0",
    "stylelint-config-standard": "^22.0"
  },
  "engines": {
    "node": ">=12",
    "npm": "^6.14",
    "yarn": "^1.22"
  },
  "peerDependencies": {
    "typescript": "^4.3",
    "vue": "^2.6"
