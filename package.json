    "prepare": "husky install"
  },
  "lint-staged": {
    "*.{js,vue}": "eslint --ext .js,.vue --ignore-path .gitignore . --fix",
    "*.{css,vue}": "stylelint **/*.{vue,css} --ignore-path .gitignore --fix"
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.13",
    "@nuxtjs/pwa": "^3.3",
    "@nuxtjs/google-adsense": "^1.3",
    "@nuxtjs/google-analytics": "^2.4",
    "@nuxtjs/strapi": "^0.3",
    "core-js": "^3.15.1",
    "date-fns": "^2.23",
    "hint.css": "^2.6",
    "lodash-es": "^4.17",
    "numeral": "^2.0",
    "nuxt": "^2.15",
    "ordinal-numbers": "^1.0",
    "postcss-comment": "^2.0",
    "postcss-custom-properties": "^10.0",
    "postcss-import": "^14.0",
    "postcss-nested": "^4.2",
    "reset-css": "^5.0"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.14",
    "@nuxt/types": "^2.15",
    "@nuxt/typescript-build": "^2.1",
